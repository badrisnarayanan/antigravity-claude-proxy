{
  "name": "antigravity-claude-proxy",
  "version": "1.0.2",
  "description": "Proxy server to use Antigravity's Claude models with Claude Code CLI",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "antigravity-claude-proxy": "bin/cli.js"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "tsc",
    "start": "tsx src/index.ts",
    "start:prod": "node dist/index.js",
    "dev": "tsx --watch src/index.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "accounts": "tsx src/cli/accounts.ts",
    "accounts:add": "tsx src/cli/accounts.ts add",
    "accounts:list": "tsx src/cli/accounts.ts list",
    "accounts:remove": "tsx src/cli/accounts.ts remove",
    "accounts:verify": "tsx src/cli/accounts.ts verify",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:bench": "vitest bench",
    "test:integration": "node tests/integration/run-all.cjs",
    "test:signatures": "node tests/integration/test-thinking-signatures.cjs",
    "test:multiturn": "node tests/integration/test-multiturn-thinking-tools.cjs",
    "test:streaming": "node tests/integration/test-multiturn-thinking-tools-streaming.cjs",
    "test:interleaved": "node tests/integration/test-interleaved-thinking.cjs",
    "test:images": "node tests/integration/test-images.cjs",
    "test:caching": "node tests/integration/test-caching-streaming.cjs",
    "test:crossmodel": "node tests/integration/test-cross-model-thinking.cjs",
    "test:oauth": "node tests/integration/test-oauth-no-browser.cjs",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "claude",
    "anthropic",
    "antigravity",
    "proxy",
    "vertex-ai"
  ],
  "author": "Badri Narayanan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/badri-s2001/antigravity-claude-proxy.git"
  },
  "homepage": "https://github.com/badri-s2001/antigravity-claude-proxy#readme",
  "bugs": {
    "url": "https://github.com/badri-s2001/antigravity-claude-proxy/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "better-sqlite3": "^12.5.0",
    "boxen": "^8.0.1",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "picocolors": "^1.1.1",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.3",
    "@vitest/coverage-v8": "^4.0.16",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "eslint": "^9.39.2",
    "fast-check": "^4.5.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.51.0",
    "vitest": "^4.0.16"
  }
}
